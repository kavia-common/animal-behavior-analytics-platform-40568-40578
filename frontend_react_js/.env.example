# Environment variables required for the VizAI React frontend.
# Copy this file to .env and adjust values for your environment.
# Note: Do not commit actual .env files with secrets.

# Base URL of the backend FastAPI service, e.g. https://localhost:3001 or http://127.0.0.1:3001
REACT_APP_API_BASE=

# Optional alternative backend URL variables (the app will fall back in order)
REACT_APP_BACKEND_URL=
REACT_APP_WS_URL=

# Frontend public URL (used for CORS in backend)
REACT_APP_FRONTEND_URL=

# Runtime config
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz

# Feature flags
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
