# Example environment variables for VizAI React frontend
# Copy to .env and adjust values as needed.

# Base URL for the backend FastAPI service (e.g., http://localhost:3001 or a preview URL)
REACT_APP_API_BASE=http://localhost:3001

# Optional alternative URLs
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=ws://localhost:3001/ws/events

# Node env flags and options
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=false
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
